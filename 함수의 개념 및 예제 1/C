#include <stdio.h>
// 두 수를 더하는 함수를 생성
int getsum (int fisrt, int second) {
    // 두 수를 더한뒤 return 해줌
    return fisrt + second;
}

// 메인 함수 실행(프로그램 실행)
int main() {
    // 두수를 입력할 변수와 결과 값을 저장할 변수를 선언
    int a, b;
    int result; 
    
    // 더할 두 수를 입력 받음 
    scanf("%d %d", &a, &b); 
   
    // getsum 함수를 통해 결과 값을 저장 
    result = getsum(a, b); 
    
    // 결과값 출력
    printf("%d\n", result);
    
    return 0;
}
