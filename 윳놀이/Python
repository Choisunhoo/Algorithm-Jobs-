# 윳놀이 

for _ in range(3):
    data = list(map(int, input().split()))
    zero = 0 
    for j in range(4): 
        if data[j] == 0:
            zero += 1
    if zero == 0:
        print("E")
    elif zero == 1:
        print("A")
    elif zero == 2:
        print("B")
    elif zero == 3:
        print("C")
    else:
        print("D")
