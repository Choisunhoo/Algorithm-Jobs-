#include <stdio.h>
// strlen을 사용하기 위해 #include <string.h> 함수를 써준다 
#include <string.h>

int main() {
  // 문자열의 저장할 변수와 최대 범위를 입력 
  char c[1005];
  // fgets(넣을곳, 최대범위, 입력할것) 을 통해서 원하는 문자열을 입력 
  fgets(c,1005,stdin);
  // 문자열의 길이를 저장하는 len 변수 생성 (문자열의 길이는 strlen(알고싶은 문자열))
  int len = strlen(c);
  // 반복문을 통해서 소문자를 대문자로 대문자를 소문자로 바꿈
  for(int i=0; i<len; i++){
    // c언어에서는 다른 변화 없이도 문자에 숫자를 더하면 아스키 코드를 통해서 문자를 바꾸는게 가능하다
    // 만약 아스키 코드값이 65~90 인경우에는 A~Z임으로 32를 더해서 a~z 로 바꿔준다 
    if(c[i] >=65 && c[i]<=90){
      c[i] +=32;
    }
    // 만약 아스키 코드값이 97~122 인경우에는 a~z임으로 32를 빼서 A~Z 로 바꿔준다 
    else if(c[i] >=97 && c[i]<=122){
       c[i]-=32;
    }
    // 그리고 변환 된 값을 출력 한다
    printf("%c", c[i]);
  }
  return 0;
}
