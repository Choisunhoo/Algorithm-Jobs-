#include <stdio.h>
#include <string.h>

int main() {
    // 문자열을 저장할 변수 생성
    char arr[1005];
    // 문자열을 입력
    fgets(arr,1005,stdin);
    // 문자열의 길이를 저장할 변수 생성 
    int len = strlen(arr);
    
    // 반복문을 통해서 문자열의 끝부터 앞까지 출력
    for(int i=len-1; i >= 0; i--){
          printf("%c", arr[i]);
    }
    return 0; 
}
