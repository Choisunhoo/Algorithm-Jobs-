#include <stdio.h>
int main(){
    int one = 0, two = 0;
    int array_1[10];
    for (int i = 0; i <10; i++){
        scanf("%d", &array_1[i]);
    }
    int array_2[10];
    for (int i = 0; i <10; i++){
        scanf("%d", &array_2[i]);
    }
    for (int i = 0; i <10; i++){
        if (array_1[i] > array_2[i]) one++;
        else if (array_1[i] < array_2[i]) two++;
    }
    if (one > two) printf("A");
    else if (one < two) printf("B");
    else printf("D");
    
    return 0;
}
